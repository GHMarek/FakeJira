
@{
    ViewBag.Title = "Index";
}

<h2>Admin Overview</h2>

<div class="row">
    <div class="col-sm-4">
        <h2>Setup Data</h2>
        <p>
            Method AppDataSetup will populate empty database tables.
        </p>
        <p>
            <a type="button" class="btn btn-primary btn-sm" href="@Url.Action("DataSetup")">
                <span class="glyphicon glyphicon-plus"></span>
                <span>Populate db</span>
            </a>
        </p>
    </div>
    <div class="col-sm-1">
        @if (ViewBag.DataSetup != null)
        {
            @ViewBag.DataSetup;
        }
    </div>
    <div class="col-sm-7"></div>
</div>